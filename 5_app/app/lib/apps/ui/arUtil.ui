 */

QWidget {
color: rgb(240, 240, 240);  /* push btn text color */
background-color: rgb(24, 25, 28);  /* widgets background color */
}

/* push btn */
QPushButton:hover {background-color: rgb(31, 49, 0); }
QPushButton:checked {background-color: rgb(71, 89, 30);}
QPushButton:disabled {color: rgb(100, 100, 100);} /* push btn text color, when disabled*/
QPushButton { color: rgb(240, 240, 240);}

/* tabs */
QTabBar::tab:hover {background-color: rgb(31, 49, 0); }
QTabBar::tab:selected  {background-color: rgb(71, 89, 30);}

/* line 
4 is for horizontal line (5 for vertical line)*/
QFrame[frameShape=&quot;4&quot;]{
background-color: rgb(240, 240, 240);
max-height: 1px;
border: none;
}
QFrame[frameShape=&quot;5&quot;]{
background-color: rgb(240, 240, 240);
max-width: 1px;
border: none;
}

/* scroll bar */
QScrollBar:vertical {
    background: transparent;
    width: 15px;
}

/* list widget */
QListWidget::item:disabled {color: rgb(100, 100, 100); }

/* RADIO BUTTONS */
QRadioButton::indicator {
width: 14px;
height: 14px;
}
/* Unchecked circle */
QRadioButton::indicator:unchecked {
border: 1px solid #666;
border-radius: 7px;
background: none;
}
/* Checked dot */
QRadioButton::indicator:checked {
border: 1px solid #666;
border-radius: 7px;
background-color: rgb(71, 89, 30);  /* DOT COLOR HERE */
}

</string>
